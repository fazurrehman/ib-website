    <!-- p-information -->
    <div class="col-md-5 col-lg-4 col-xl-4">
      <div class="order-information">
        <h1 class="product__title">
          Diamond and Square Ring in 18k Rose Gold Color
        </h1>
        <!-- pricing -->
        <div class="product__price">
          <span class="product__price-value">$ 350</span>
          <span class="product__stock-inventory --green">In Stock</span>
          <span class="product__stock-inventory --red">Only 2 items left</span>
        </div>
        <!-- colors -->
        <div class="product__color">
          <h4 class="product-subtitle">
            Color <span class="color-selected">Rose Gold</span>
          </h4>
          <ul class="product__color-choose">
            <li>
              <input type="radio" name="colour" id="golden" class="check_in">
              <label for="golden"></label>
              <div class="check color--golden"></div>
            </li>
            <li>
              <input type="radio" name="colour" id="silver" class="check_in">
              <label for="silver"></label>
              <div class="check color--silver"></div>
            </li>
            <li>
              <input type="radio" name="colour" id="rosegold" class="check_in">
              <label for="rosegold"></label>
              <div class="check color--rose"></div>
            </li>
            <li>
              <input type="radio" name="colour" id="twotone-wy" class="check_in">
              <label for="twotone-wy"></label>
              <div class="check color--twotone-wy"></div>
            </li>
            <li>
