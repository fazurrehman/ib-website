              <label for="twotone-wr"></label>
              <div class="check color--twotone-wr"></div>
            </li>
            <li>
              <input type="radio" name="colour" id="triTone" class="check_in">
              <label for="triTone"></label>
              <div class="check color--tritone"></div>
            </li>
            <li>
              <input type="radio" name="colour" id="colorBlue" class="check_in">
              <label for="colorBlue"></label>
              <div class="check color--blue"></div>
            </li>
          </ul>
        </div>
        <!-- end product__color -->
        <!-- diamond quality -->
        <div class="diamond margin-b-16">
          <h4 class="product-subtitle">Diamond Quality</h4>
          <ul class="diamond__choose">
            <li>
              <div class="icon-m"><img src="img/icons/si.svg" alt=""></div>
              <span class="diamond-title">SI</span>
            </li>
            <li>
              <div class="icon-m"><img src="img/icons/vs.svg" alt=""></div>
              <span class="diamond-title">VS</span>
            </li>
            <li>
              <div class="icon-m"><img src="img/icons/vvs.svg" alt=""></div>
              <span class="diamond-title">VVS</span>
            </li>
          </ul>
        </div>
        <!-- end diamond quality -->
        <!-- add neckless -->
        <div class="necklace__cta" data-toggle="modal" data-target="#accessoriesModal">
          <div class="necklace__cta-icon icon--add-plus"></div>
          <div class="necklace__cta-info">
            <div class="necklace__cta-info-title">Add Necklace</div>
            <span class="necklace__cta-info-overline">We’ve best matching necklaces for you</span>
