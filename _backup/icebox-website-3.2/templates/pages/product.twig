                <li><a href="#">Medium</a></li>
                <li><a href="#">Large</a></li>
                <li><a href="#">Extra Large</a></li>
              </ul>
            </div>
          </div>
          <!-- end dropdown -->
          <div class="container-options__dropdown">
            <div class="container-options__dropdown_wrapper">
              <div class="container-options__dropdown_wrapper_container">
                <h4 class="container-options__dropdown_wrapper_title">
                  Earrning Options
                </h4>
              </div>
              <button class="btn container-options__dropdown_wrapper_button" type="button" data-toggle="collapse"
                data-target="#option-1" aria-expanded="false" aria-controls="option-1">
                Pair<i class="icon--arrowdown"></i>
              </button>
            </div>
            <div class="collapse container-options__collapse" id="option-1">
              <ul class="container-options__collapse_options">
                <li><a href="#">Pair</a></li>
                <li><a href="#">Single</a></li>
              </ul>
            </div>
          </div>
          <!-- end dropdown -->
          <div class="container-options__dropdown">
            <div class="container-options__dropdown_wrapper">
              <div class="container-options__dropdown_wrapper_container">
                <h4 class="container-options__dropdown_wrapper_title">
                  Necklace Length
                </h4>
                <a href="#">Size Guide</a>
              </div>
              <button class="btn container-options__dropdown_wrapper_button" type="button" data-toggle="collapse"
