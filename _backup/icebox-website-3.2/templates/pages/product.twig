                  <li>
                    <span class="title">Icebox</span>
                    <label>Jewelry Brand</label>
                  </li>
                  <li>
                    <span class="title">Unisex</span>
                    <label>Gender</label>
                  </li>
                  <li>
                    <span class="title">14K</span>
                    <label>Metal Purity</label>
                  </li>
                  <li>
                    <span class="title">0.27ctw</span>
                    <label>Total Diamond Weight</label>
                  </li>
                </ul>
              </div>
            </div>
          </div>
          <!-- end of accordion item -->
        </div>
        <!-- end of accordion -->
        <!-- Button -->
        <button class="btn btn-ask-question btn-secondary" href="#" target="_blank" role="button">
          <span>Ask a question</span>
        </button>
      </div>
